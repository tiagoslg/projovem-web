

<ui:composition
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ui="http://java.sun.com/jsf/facelets"
    xmlns:h="http://java.sun.com/jsf/html"
    xmlns:f="http://java.sun.com/jsf/core"
    xmlns:a4j="http://richfaces.org/a4j"
    xmlns:rich="http://richfaces.org/rich">

    <div style="width:1000px;padding:10px;border:1px solid">
        <div style="border-bottom:2px solid;height:150px;margin-bottom:30px;">
            <div style="width:250px;float:left;padding-top:15px;">
                <a HREF="relatoriocoordenacao.jsf"><!--p><img alt="Projovem Urbano" src="imagens/logo-projovem.JPG" border="0" style="width: 250px; height: 90px;"/></p--></a>

            </div>
            <div style="width:500px;float:left">
                <h1 style="margin-bottom:0;margin-left:0;margin-right:0;margin-top:10px;text-align:center;">PROJOVEM URBANO</h1>
                <h2 style="margin-bottom:0;margin-left:0;margin-right:0;margin-top:10px;text-align:center;">RELATÓRIO SÍNTESE DE SUPERVISÃO</h2>
                <h3 style="margin-bottom:0;margin-left:0;margin-right:0;margin-top:10px;text-align:center;">Rotina 2 - ROTINA DE FORMAÇÃO INICIAL</h3>
                <h4 style="margin-bottom:0;margin-left:0;margin-right:0;margin-top:10px;text-align:center;">PERFIL DOS FORMADORES</h4>
            </div>
            <div style="width:200px;float:left;padding-top:50px;padding-left:30px">
                <!--p><img alt="Brasil, um país de todos" src="imagens/brasil_gd.jpg"/></p-->
            </div>
        </div>


        <table width="100%">
            <tr>
                <td>
                    <h:outputText value="Instituição responsável pela supervisão do programa #{sinteseSupervisaoBean.irResponsavel}"/>
                </td>
                <td rowspan="2" width="10%">
                    <a target="_self" HREF="javaScript:window.print()"><img alt="Imprimir" border="0" src="imagens/impressora.gif"/></a>
                </td>
            </tr>
            <tr>
                <td>
                    <p><h:outputText value="Dados referentes à #{sinteseSupervisaoBean.coordenacaoEscolhida}"/></p>
                </td>
            </tr>
        </table>


        <center>
            <rich:dataTable

                onRowMouseOver="this.style.backgroundColor='#FCF6EE'"
                onRowMouseOut="this.style.backgroundColor='#{a4jSkin.tableBackgroundColor}'"
                cellpadding="0" cellspacing="0"
                border="0" var="rotina" value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.listaRotina002PerfilFormadores}">


                <rich:column >
                    <f:facet name="header">
                        <h:outputText value="ID"/>
                    </f:facet>
<<<<<<< .mine
                    <CENTER><h:outputText value="#{rotina.id}"/></CENTER>
=======
                    <center><h:outputText value="#{rotina.id}"/></center>
>>>>>>> .r145
                </rich:column>
                 <rich:column >
                    <f:facet name="header">
                        <h:outputText value="Município"/>
                    </f:facet>
                    <h:outputText value="#{rotina.identificaEntradaMunicipio}"/>
                </rich:column>
                <rich:column >
                    <f:facet name="header">
                        <h:outputText value="Data Observação"/>
                    </f:facet>
<<<<<<< .mine
                    <CENTER><h:outputText value="#{rotina.dataInsercao}"/></CENTER>
=======
                    <center><h:outputText value="#{rotina.dataInsercao}"/></center>
>>>>>>> .r145
                </rich:column>
            </rich:dataTable>
        </center>

        <p> <h:outputText value=" #{sinteseSupervisaoBean.listaRotina002PerfilFormadoresSize} formadores entrevistados."/></p>
        <br/>

        <p><b><h:outputText value="Aspectos observados que acrescentam às condicionalidades estabelecidas no programa e contribuem para sua qualidade:"/></b></p>

        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4i}% dos formadores tem Mestrado como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4i gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4j}% dos formadores tem Doutorado ou posterior como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4j gt 0}"/> </p>

        <p><h:outputText value="Os dados registrados não contém observações a este respeito."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.positivo eq 0}"/> </p>

        <br/>

        <p><b><h:outputText value="Aspectos observados quanto ao não cumprimento das condicionalidades estabelecidas no programa e que afetam sua implementação:"/></b></p>

        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4a}% dos formadores tem o ensino fundamental como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4a gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4b}% dos formadores tem o ensino médio/técnico como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4b gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4c}% dos formadores tem o ensino médio/magistério como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4c gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4d}% dos formadores tem o ensino Superior Incompleto como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4d gt 0}"/> </p>

        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q6a}% dos formadores não concluiu o ensino superior."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q6a gt 0}"/> </p>

        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q7b}% dos formadores participaram parcialmente da formação de formadores."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q7b gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q7c}% dos formadores não participaram da formação de formadores."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q7c gt 0}"/> </p>

        <p><h:outputText value="Os dados registrados não contém observações a este respeito."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.negativo eq 0}"/> </p>

        <br/>

        <!--p><b><h:outputText value="Outros aspectos observados:"/></b></p>
        
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4e}% dos formadores tem o ensino Superior completo tecnológico como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4e gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4f}% dos formadores tem o ensino Superior completo licenciatura como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4f gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4g}% dos formadores tem o ensino Superior completo bacharelado como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4g gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4h}% dos formadores tem Especialização (lato sensu) como o nível máximo de escolaridade"
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q4h gt 0}"/> </p>

        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q6b}% dos formadores tem como formação o curso Tecnólogo."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q6b gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q6c}% dos formadores tem formação em Educação (Pedagogia, Normal Superior ou similar)."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q6c gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q6d}% dos formadores tem formação em letras (ou similar)."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q6d gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q6e}% dos formadores tem formação em Ciências Humanas (História, Geografia ou similar)."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q6e gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q6f}% dos formadores tem formação em Ciências Exatas (Matemática, Física, Química ou similar)."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q6f gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q6g}% dos formadores tem formação em Ciências Biológicas (Ciências, Biologia ou similar)."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q6g gt 0}"/> </p>

        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q7a}% dos formadores participaram da formação de formadores."
                         rendered="#{sinteseSupervisaoBean.contagem002PerfilFormadores.q7a gt 0}"/> </p>

        </p-->
       
        <br/>
        <p><h:outputText value="Sr(a) Coordenador(a), este é um relatório dinâmico, novos dados poderão ser inseridos ou atualizados e alterar os resultados apresentados."/></p>
        <p><h:outputText value="Brasília, #{sinteseSupervisaoBean.contagem002PerfilFormadores.dataatual}"/></p>
        <hr/>
        <p align="center" id="rodape">Assessoria de Avaliação e Supervisão da Coordenação Nacional do Projovem Urbano<br/>
            <a href="mailto:avaliacao.supervisaoprojovem@planalto.gov.br">avaliacao.supervisaoprojovem@planalto.gov.br</a></p>
        <p align="right" id="des">Desenvolvido pela UFMG</p>
    </div>
</ui:composition>
