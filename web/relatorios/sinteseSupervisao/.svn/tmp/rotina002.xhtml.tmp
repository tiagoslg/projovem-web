

<ui:composition
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ui="http://java.sun.com/jsf/facelets"
    xmlns:h="http://java.sun.com/jsf/html"
    xmlns:f="http://java.sun.com/jsf/core"
    xmlns:a4j="http://richfaces.org/a4j"
    xmlns:rich="http://richfaces.org/rich">

    <div style="width:1000px;padding:10px;border:1px solid">
        <div style="border-bottom:2px solid;height:150px;margin-bottom:30px;">
            <div style="width:250px;float:left;padding-top:15px;">
                <a HREF="relatoriocoordenacao.jsf"><!--p><img alt="Projovem Urbano" src="imagens/logo-projovem.JPG" border="0" style="width: 250px; height: 90px;"/></p--></a>

            </div>
            <div style="width:500px;float:left">
                <h1 style="margin-bottom:0;margin-left:0;margin-right:0;margin-top:10px;text-align:center;">PROJOVEM URBANO</h1>
                <h2 style="margin-bottom:0;margin-left:0;margin-right:0;margin-top:10px;text-align:center;">RELATÓRIO SÍNTESE DE SUPERVISÃO</h2>
                <h3 style="margin-bottom:0;margin-left:0;margin-right:0;margin-top:10px;text-align:center;">Rotina 2 - ROTINA DE FORMAÇÃO INICIAL</h3>
            </div>
            <div style="width:200px;float:left;padding-top:50px;padding-left:30px">
                <!--p><img alt="Brasil, um país de todos" src="imagens/brasil_gd.jpg"/></p-->
            </div>
        </div>


        <table width="100%">
            <tr>
                <td>
                    <h:outputText value="Instituição responsável pela supervisão do programa #{sinteseSupervisaoBean.irResponsavel}"/>
                </td>
                <td rowspan="2" width="10%">
                    <a target="_self" HREF="javaScript:window.print()"><img alt="Imprimir" border="0" src="imagens/impressora.gif"/></a>
                </td>
            </tr>
            <tr>
                <td>
                    <p><h:outputText value="Dados referentes à #{sinteseSupervisaoBean.coordenacaoEscolhida}"/></p>
                </td>
            </tr>
        </table>


        <center>
            <rich:dataTable

                onRowMouseOver="this.style.backgroundColor='#FCF6EE'"
                onRowMouseOut="this.style.backgroundColor='#{a4jSkin.tableBackgroundColor}'"
                cellpadding="0" cellspacing="0"
                border="0" var="rotina" value="#{sinteseSupervisaoBean.contagem002.listaRotina002}">


                <rich:column >
                    <f:facet name="header">
                        <h:outputText value="ID"/>
                    </f:facet>
<<<<<<< .mine
                    <CENTER><h:outputText value="#{rotina.id}"/></CENTER>
=======
                    <center><h:outputText value="#{rotina.id}"/></center>
>>>>>>> .r145
                </rich:column>
                 <rich:column >
                    <f:facet name="header">
                        <h:outputText value="Município"/>
                    </f:facet>
                    <h:outputText value="#{rotina.identificaEntradaMunicipio}"/>
                </rich:column>
                <rich:column >
                    <f:facet name="header">
                        <h:outputText value="Data Observação"/>
                    </f:facet>
<<<<<<< .mine
                    <CENTER><h:outputText value="#{rotina.dataInsercao}"/></CENTER>
=======
                    <center><h:outputText value="#{rotina.dataInsercao}"/></center>
>>>>>>> .r145
                </rich:column>
            </rich:dataTable>
        </center>

        <p> <h:outputText value=" #{sinteseSupervisaoBean.listaRotina002Size} locais observados."/></p>
        <br/>

        <p><b><h:outputText value="Aspectos observados que acrescentam às condicionalidades estabelecidas no programa e contribuem para sua qualidade:"/></b></p>

        <p><h:outputText value="Em #{sinteseSupervisaoBean.contagem002.QNumeroEducadoresIgual} dos núcleos observados estavam presentes todos os educadores inscritos."
                         rendered="#{sinteseSupervisaoBean.contagem002.QNumeroEducadoresIgual gt 0}"/> </p>

        <p><h:outputText value="O horário de funcionamento de #{sinteseSupervisaoBean.contagem002.q9Superior} dos núcleos observados, no dia da visita, foi superior a 4 horas."
                         rendered="#{sinteseSupervisaoBean.contagem002.q9Superior gt 0}"/> </p>

        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q18Mestrado}% dos Educadores tem Mestrado como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002.q18Mestrado gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q18Doutorado}% dos Educadores tem Doutorado ou posterior como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002.q18Doutorado gt 0}"/> </p>

        <p><h:outputText value="Os dados registrados não contém observações a este respeito."
                         rendered="#{sinteseSupervisaoBean.contagem002.positivo eq 0}"/> </p>

        <br/>

        <p><b><h:outputText value="Aspectos observados quanto ao não cumprimento das condicionalidades estabelecidas no programa e que afetam sua implementação:"/></b></p>

        <p><h:outputText value="Em #{sinteseSupervisaoBean.contagem002.QNumeroFormadoresDiferente} dos núcleos observados não estavam presentes todos os formadores contratados."
                         rendered="#{sinteseSupervisaoBean.contagem002.QNumeroFormadoresDiferente gt 0}"/> </p>

        <p><h:outputText value="O horário de funcionamento de #{sinteseSupervisaoBean.contagem002.q9Entre} dos núcleos observados, no dia da visita, foi entre 3 e 4 horas."
                         rendered="#{sinteseSupervisaoBean.contagem002.q9Entre gt 0}"/> </p>
        <p><h:outputText value="O horário de funcionamento de #{sinteseSupervisaoBean.contagem002.q9Inferior} dos núcleos observados, no dia da visita, foi inferior a 3 horas."
                         rendered="#{sinteseSupervisaoBean.contagem002.q9Inferior gt 0}"/> </p>

        <p><h:outputText value="Em #{sinteseSupervisaoBean.contagem002.q10b} dos núcleos observados os horários não se repetiram todos os dias desde o início do curso."
                         rendered="#{sinteseSupervisaoBean.contagem002.q10b gt 0}"/> </p>

        <p><h:outputText value="Em #{sinteseSupervisaoBean.contagem002.q11b} dos núcleos observados o guia do Educador não foi utilizado."
                         rendered="#{sinteseSupervisaoBean.contagem002.q11b gt 0}"/> </p>

        <p><h:outputText value="Em #{sinteseSupervisaoBean.contagem002.q12b} dos núcleos observados o Manual do aluno não foi utilizado."
                         rendered="#{sinteseSupervisaoBean.contagem002.q12b gt 0}"/> </p>

        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q18Fundamental}% dos Educadores tem o ensino fundamental como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002.q18Fundamental gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q18MedioTecnico}% dos Educadores tem o ensino médio/técnico como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002.q18MedioTecnico gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q18MedioMagisterio}% dos Educadores tem o ensino médio/magistério como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002.q18MedioMagisterio gt 0}"/> </p>

        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q19Superior}% dos Educadores não concluiu o ensino superior."
                         rendered="#{sinteseSupervisaoBean.contagem002.q19Superior gt 0}"/> </p>

        <p><h:outputText value="Os dados registrados não contém observações a este respeito."
                         rendered="#{sinteseSupervisaoBean.contagem002.negativo eq 0}"/> </p>

        <br/>

        <!--p><b><h:outputText value="Outros aspectos observados:"/></b></p>
        
        <p><h:outputText value="Em #{sinteseSupervisaoBean.contagem002.qnumeroeducadoresdiferente} dos núcleos observados não estavam presentes todos os educadores inscritos."
                         rendered="#{sinteseSupervisaoBean.contagem002.qnumeroeducadoresdiferente gt 0}"/> </p>

        <p><h:outputText value="Em #{sinteseSupervisaoBean.contagem002.qnumeroformadoresigual} dos núcleos observados estavam presentes todos os formadores contratados."
                         rendered="#{sinteseSupervisaoBean.contagem002.qnumeroformadoresigual gt 0}"/> </p>

        <p><h:outputText value="O horário de funcionamento de #{sinteseSupervisaoBean.contagem002.q9igual} dos núcleos observados, no dia da visita, foi igual a 4 horas."
                         rendered="#{sinteseSupervisaoBean.contagem002.q9igual gt 0}"/> </p>

        <p><h:outputText value="Em #{sinteseSupervisaoBean.contagem002.q10a} dos núcleos observados os horários se repetiram todos os dias desde o início do curso."
                         rendered="#{sinteseSupervisaoBean.contagem002.q10a gt 0}"/> </p>

        <p><h:outputText value="Em #{sinteseSupervisaoBean.contagem002.q11a} dos núcleos observados o guia do Educador foi utilizado."
                         rendered="#{sinteseSupervisaoBean.contagem002.q11a gt 0}"/> </p>

        <p><h:outputText value="Em #{sinteseSupervisaoBean.contagem002.q12a} dos núcleos observados o Manual do aluno foi utilizado."
                         rendered="#{sinteseSupervisaoBean.contagem002.q12a gt 0}"/> </p>

        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q18superiorincompleto}% dos Educadores o ensino Superior Incompleto como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002.q18superiorincompleto gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q18superiorcompletotecnologico}% dos Educadores tem o ensino Superior completo tecnológico como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002.q18superiorcompletotecnologico gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q18superiorcompletolicenciatura}% dos Educadores tem o ensino Superior completo licenciatura como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002.q18superiorcompletolicenciatura gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q18superiorcompletobacharelado}% dos Educadores tem o ensino Superior completo bacharelado como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002.q18superiorcompletobacharelado gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q18especializacao}% dos Educadores tem Especialização (lato sensu) como o nível máximo de escolaridade."
                         rendered="#{sinteseSupervisaoBean.contagem002.q18especializacao gt 0}"/> </p>

        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q19tecnologo}% dos Educadores tem como formação o curso Tecnólogo."
                         rendered="#{sinteseSupervisaoBean.contagem002.q19tecnologo gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q19educacao}% dos Educadores tem formação em Educação (Pedagogia, Normal Superior ou similar)."
                         rendered="#{sinteseSupervisaoBean.contagem002.q19educacao gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q19letras}% dos Educadores tem formação em letras (ou similar)."
                         rendered="#{sinteseSupervisaoBean.contagem002.q19letras gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q19humanas}% dos Educadores tem formação em Ciências Humanas (História, Geografia ou similar)."
                         rendered="#{sinteseSupervisaoBean.contagem002.q19humanas gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q19exatas}% dos Educadores tem formação em Ciências Exatas (Matemática, Física, Química ou similar)."
                         rendered="#{sinteseSupervisaoBean.contagem002.q19exatas gt 0}"/> </p>
        <p><h:outputText value="#{sinteseSupervisaoBean.contagem002.q19biologicas}% dos Educadores tem formação em Ciências Biológicas (Ciências, Biologia ou similar)."
                         rendered="#{sinteseSupervisaoBean.contagem002.q19biologicas gt 0}"/> </p>
         </p-->
       
        <br/>
        <p><h:outputText value="Sr(a) Coordenador(a), este é um relatório dinâmico, novos dados poderão ser inseridos ou atualizados e alterar os resultados apresentados."/></p>
        <p><h:outputText value="Brasília, #{sinteseSupervisaoBean.contagem002.dataatual}"/></p>
        <hr/>
        <p align="center" id="rodape">Assessoria de Avaliação e Supervisão da Coordenação Nacional do Projovem Urbano<br/>
            <a href="mailto:avaliacao.supervisaoprojovem@planalto.gov.br">avaliacao.supervisaoprojovem@planalto.gov.br</a></p>
        <p align="right" id="des">Desenvolvido pela UFMG</p>
    </div>
</ui:composition>
