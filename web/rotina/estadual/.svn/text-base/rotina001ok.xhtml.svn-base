
<ui:composition
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ui="http://java.sun.com/jsf/facelets"
    xmlns:h="http://java.sun.com/jsf/html"
    xmlns:f="http://java.sun.com/jsf/core"
    xmlns:a4j="http://richfaces.org/a4j"
    xmlns:rich="http://richfaces.org/rich"
    xmlns:c="http://java.sun.com/jstl/core"
    template="/basic.xhtml">

    <ui:define  name="header">

        <div class="logo">

        </div>

        <div class="clr"></div>
        <div class="menu">
            <h:form>
                 <ul>
                    <li><a href="http://projovem.fae.ufmg.br/projovem/index.jsf" >Home<br />
                            <span>página inicial</span></a></li>
                    <li><a href="http://projovem.fae.ufmg.br/projovem/rotina.jsf" class="active">Rotinas<br />
                            <span>lançar respostas</span></a></li>
                    <li><a href="http://projovem.fae.ufmg.br/projovem/relatorio.jsf">Relatórios<br />
                            <span>informações</span></a></li>
                    <li><a href="http://projovem.fae.ufmg.br/projovem/utilitario.jsf"  >Utilitários<br />
                            <span>meus dados</span></a></li>
                    <li><a href="http://projovem.fae.ufmg.br/projovem/contato.jsf">Contato<br />
                            <span>fale conosco</span></a></li>
                    <li><h:commandLink action="#{loginBean.sair}">Sair<br />
                        <span>sair do sistema</span></h:commandLink></li>
                </ul>
            </h:form>
        </div>
        <div class="clr"></div>
    </ui:define>

    <ui:define  name="body">

        <div class="inner">
            <span class="h1">
                <h:outputText style="font-weight: bold; float: left; margin-top: 30px; margin-left: 310px;" value="O comprovante de lançamento será emitido somente após a validação dos dados. " rendered="#{rotinasLancadasBean.cerotina001.dadosValidos eq -1 || rotinasLancadasBean.cerotina001.dadosValidos eq 0}"/>
                <h:outputText style="font-weight: bold; float: left; margin-top: 30px; margin-left: 310px;" value="Comprovante de Inserção de dados " rendered="#{rotinasLancadasBean.cerotina001.dadosValidos eq 1}"/>
                <h:outputLink value="javaScript:window.print()" style="float: left;" target="_self" rendered="#{rotinasLancadasBean.cerotina001.dadosValidos eq 1}">
                    <h:graphicImage alt="Imprimir" border="0" width="60px" height="40px" value="http://projovem.fae.ufmg.br/projovem/imagens/impressora.gif"/>
                </h:outputLink>
            </span>
            <br/>
            <p style="display:inline-block;">
                <h:outputText  value="O supervisor(a)  "/>
                <h:outputText  value="#{rotinasLancadasBean.usuarioResponsavel}"/>
                <h:outputText  value="#{CeRotina001Bean.usuarioResponsavel}"/>
                <h:outputText  value=", CPF: "/>
                <h:outputText  value="#{rotinasLancadasBean.cpfResponsavel}"/>
                <h:outputText  value="#{CeRotina001Bean.cpfResponsavel}"/>
                <h:outputText  value=" realizou a inserção de dados para a rotina
                               de supervisão das condições gerais dos núcleos (Rotina 001 CE) no dia "/>
                <h:outputText  value="#{rotinasLancadasBean.cerotina001.dataInsercao} às "/>
                <h:outputText  value="#{rotinasLancadasBean.cerotina001.horaInsercao}."/>
            </p>
            <p style="display:inline-block;">
                <c:if test="#{rotinasLancadasBean.cerotina001.dadosEditados eq 1}">
                    <h:outputText  value="Esta rotina foi editada pelo usuário  "/>
                    <h:outputText  value="#{rotinasLancadasBean.usuarioEditor}"/>
                    <h:outputText  value=" no dia "/>
                    <h:outputText  value="#{rotinasLancadasBean.cerotina001.dataEdicao} às "/>
                    <h:outputText  value="#{rotinasLancadasBean.cerotina001.horaEdicao}."/>
                </c:if>
            </p>
            <br/>
            <br/>
            <h4>ROTINA GERAL DOS NÚCLEOS</h4>

            <h2><h:outputText value="CÓDIGO DO INSTRUMENTO = CE001"/></h2>

            <p><strong>Nº de Identificação: <h:outputText value=" #{rotinasLancadasBean.cerotina001.id}" /></strong></p>
            <p>UF: <h:outputText value=" #{rotinasLancadasBean.cerotina001.nucleo.uf}" /></p>
            <p>Coordenação: <h:outputText value=" #{rotinasLancadasBean.cerotina001.nucleo.coordenacao}" /></p>
            <p>Município: <h:outputText value=" #{rotinasLancadasBean.cerotina001.nucleo.municipio}" /></p>
            <p>Entrada: <h:outputText value=" #{rotinasLancadasBean.cerotina001.nucleo.entrada}" /></p>
            <br/>

            <c:if test="#{rotinasLancadasBean.cerotina001.dadosValidos eq 1}">
                <ui:include src="respostas/rotina001.xhtml" />
            </c:if>
        </div>

    </ui:define>

</ui:composition>
