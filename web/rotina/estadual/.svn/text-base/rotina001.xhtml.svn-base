        <h4>ROTINA GERAL DOS NÚCLEOS</h4>

        <div class="portfolio">
            <h2><h:outputText value="CÓDIGO DO INSTRUMENTO = CE 1"/></h2>
            <div class="text-home">
                <a4j:keepAlive beanName="CeRotina001Bean"/>

                <h:form id="form">

                    <h:inputHidden id="versaoFormulario" value="1"/>

                    <div id="erros" style="width:300px;display:inline-block;">
                        <h:messages/>
                    </div>

                    <p>Selecione a coordenação em que se encontra o local observado.</p>

                    <a4j:include viewId="/include/CeIdentificaNucleo.xhtml"/>

                    <a4j:region  rendered="#{!nucleoBean.renderedIdentificaLocal}">
