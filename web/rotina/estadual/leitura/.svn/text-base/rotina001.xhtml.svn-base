                        <table>
                            <tr>
                                <td>
                                    <p>A - Horário de abertura: </p>
                                </td>
                                <td>
                            <h:inputText id="q3a"
                                         value="#{rotinasLancadasBean.cerotina001.q3a}"
                                         size="5"
                                         maxlength="5"
                                         onkeyup="mascara_hora(this,event)"
                                         onblur="Verifica_Hora(this)"

                                         disabled  ="#{rotinasLancadasBean.disabled}"/>
                            <h:message for="q3a" style="color:#c70000; font-size:18px;"/>
                    </td>
                </tr>
                <tr>
                    <td>
                        <p>B - Horário de Fechamento: </p>
                    </td>
                    <td>
                <h:inputText id="q3b"
                             value="#{rotinasLancadasBean.cerotina001.q3b}"
                             size="5"
                             maxlength="5"
                             onkeyup="mascara_hora(this,event)"
                             onblur="Verifica_Hora(this)"

                             disabled  ="#{rotinasLancadasBean.disabled}"/>
                <h:message for="q3b" style="color:#c70000; font-size:18px;"/>
                </td>
                </tr>
            </table>
            </td>
            </tr>
            <tr>
                <td colspan="2">
