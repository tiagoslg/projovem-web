package projovem.vo;

import java.util.ArrayList;
import java.util.List;
import javax.faces.model.SelectItem;
import projovem.entity.RespostaDada;

/**
 *
 * @author admprojovem
 */
public class RespostasPossiveis {

    private List<SelectItem> questoesMonovaloradas = new ArrayList<SelectItem>();
    private List<RespostaDada> questoesTextuais = new ArrayList<RespostaDada>();

    /**
     * @return the questoesMonovaloradas
     */
    public List<SelectItem> getQuestoesMonovaloradas() {
        return questoesMonovaloradas;
    }

    /**
     * @param questoesMonovaloradas the questoesMonovaloradas to set
     */
    public void setQuestoesMonovaloradas(List<SelectItem> questoesMonovaloradas) {
        this.questoesMonovaloradas = questoesMonovaloradas;
    }

    /**
     * @return the questoesTextuais
     */
    public List<RespostaDada> getQuestoesTextuais() {
        return questoesTextuais;
    }

    /**
     * @param questoesTextuais the questoesTextuais to set
     */
    public void setQuestoesTextuais(List<RespostaDada> questoesTextuais) {
        this.questoesTextuais = questoesTextuais;
    }

}
