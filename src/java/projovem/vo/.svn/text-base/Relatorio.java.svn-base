package projovem.vo;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.List;
import projovem.entity.Questao;
import projovem.entity.Resposta;
import projovem.entity.RespostaDada;

/**
 *
 * @author admprojovem
 */
public class Relatorio implements Serializable {

        private Questao questao;
        private List<Resposta> resposta = new ArrayList<Resposta>();
        private List<RespostaDada> respostaDada = new ArrayList<RespostaDada>();

        /**
         * @return the questao
         */
        public Questao getQuestao() {
            return questao;
        }

        /**
         * @param questao the questao to set
         */
        public void setQuestao(Questao questao) {
            this.questao = questao;
        }

        /**
         * @return the resposta
         */
        public List<Resposta> getResposta() {
            return resposta;
        }

        /**
         * @param resposta the resposta to set
         */
        public void setResposta(List<Resposta> resposta) {
            this.resposta = resposta;
        }

    /**
     * @return the respostaDada
     */
    public List<RespostaDada> getRespostaDada() {
        return respostaDada;
    }

    /**
     * @param respostaDada the respostaDada to set
     */
    public void setRespostaDada(List<RespostaDada> respostaDada) {
        this.respostaDada = respostaDada;
    }

}
